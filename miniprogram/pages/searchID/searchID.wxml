<!--pages/searchID/searchID.wxml-->
<!-- 顶部自定义 -->
<cu-custom bgImage="https://image.weilanwl.com/color2.0/plugin/sylb2244.jpg" isCustom="{{true}}"><view slot="content">查询鉴定</view></cu-custom>

<!-- 搜索栏 -->
<view class="cu-bar bg-white search fixed" style="top:{{CustomBar}}px;">
  <view class="cu-avatar round">
    <text class="cuIcon-people line-cyan"></text>
  </view>
  
  <view class="search-form round">
    <text class="cuIcon-search"></text>
    <input type="text" placeholder="输入卖家的关键词" confirm-type="search"></input>
  </view>

  <view class="action">
    <button class="F">搜索</button>
  </view>
</view>
